{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js", // Trỏ đến file JS ĐÃ BUILD
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)", // Bắt tất cả các route
            "dest": "/dist/main.js", // Trỏ đến file handler ĐÃ BUILD
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS",
                "HEAD"
            ]
        }
    ]
}